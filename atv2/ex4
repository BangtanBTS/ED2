#include <bits/stdc++.h> 
#include <fstream>
using namespace std;

char mat[4][4];

void busca(){
	for(int l=0; l<4; l++){
		for(int c=0; c<4; c++){
			if(mat[l][c]=='m'){
				cout<<"linha: "<<l<<" Coluna: "<<c<<endl;
			}
		}
	}
}

int main() {
	char x; 
	//x=p -> planeta
	//x=m -> estrela da morte
	//x=e -> estrela
	for(int l=0; l<4; l++){
		for(int c=0; c<4; c++){
			cin>>x;
			mat[l][c]=x;
		}
	}
	busca();
}

/*p p p p 
p p e m
e e e e
e e p p*/
