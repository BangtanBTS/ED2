#include <bits/stdc++.h>
using namespace std;

int vet[1000];

void sort(){
	int i, key;
	for(int j=1; j<1000; j++){
		key=vet[j];
		i=j-1;
		while(i>-1 and vet[i]>key){
			vet[i+1]=vet[i];
			i=i-1;
		}
		vet[i+1]=key;
	}
}

int main()
{
	
	srand(time(0));
	double timespent = 0.0;
   for(int i=0; i<1000; i++){
	   vet[i]=rand()%2000;
   }
   clock_t begin = clock();
   sort();
   clock_t end = clock();
   
   for(int i=0; i<1000; i++){
		cout<<vet[i]<<" ";
   }
   cout<<endl;
   timespent += (double)(end - begin) / CLOCKS_PER_SEC;
   
   cout<<fixed<<setprecision(5);
   cout<<"Timer: "<<timespent;
}
