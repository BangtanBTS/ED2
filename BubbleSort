#include <bits/stdc++.h>
#include <cstdlib>
#include <cstdio>
using namespace std;

void BubbleSort(vector<pair<int,string>> &lista, int tam){
	for(int fim=tam-1; fim>0; fim--){
		for(int i=0; i<fim; i++){
			if(lista[i].first>lista[i+1].first){
				pair<int, string>aux=lista[i];
				lista[i]=lista[i+1];
				lista[i+1]=aux;
			}
		}
	}
}
