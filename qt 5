#include <bits/stdc++.h>
#include <string> 
#include <fstream>
using namespace std;

void BubbleSort(vector<string> &lista, int tam){
	for(int fim=tam-1; fim>0; fim--){
		for(int i=0; i<fim; i++){
			if(lista[i]>lista[i+1]){
				string aux=lista[i];
				lista[i]=lista[i+1];
				lista[i+1]=aux;
			}
		}
	}
}

int main() {
	vector<string> lista;
	ifstream arq;
	string linha;
	arq.open("arq.txt");
	if(arq.is_open()){
		while(getline(arq, linha)){
			cout<<linha<<endl;
			lista.push_back(stoi(linha));
		}
	}
	else{
		cout<<"Erro"<<endl;
	}
	arq.close();
	BubbleSort(lista, lista.size());
	
	ofstream arq1;
	arq1.open("arq.txt");
	for(int i=0; i<(int)lista.size(); i++){
		arq1<<lista[i];
	}
	arq1.close();
	
}
